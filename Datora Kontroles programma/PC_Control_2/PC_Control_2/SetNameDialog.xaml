<Window x:Class="PC_Control_2.SetNameDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PC_Control_2"
        mc:Ignorable="d"
        Title="SetNameDialog" Height="120" Width="300">
    <StackPanel>
        <TextBox x:Name="NameTextBox" Margin="10" Width="260"/>
        <Button Content="OK" Click="OKButton_Click" Margin="10" Width="260"/>
    </StackPanel>
</Window>
